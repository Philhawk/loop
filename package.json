  "name": "loop",
  "version": "0.0.1",
  "engines": {
    "node": ">=8.0.0"
  },
  "description": "Real-time feedback for teachers in the classroom.",
  "main": "index.js",
  "scripts": {
    "test": "bin/setup && mocha --compilers js:babel-register app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "test-watch": "bin/setup && mocha --compilers js:babel-register --watch app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "build": "webpack",
    "build-watch": "bin/setup && webpack -w",
    "start": "webpack && npm run seed && nodemon server/start.js",
    "seed": "node db/seed.js"
  },
